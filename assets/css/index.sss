// --- Product Posts

.blue-pullquote
	border-left: 2px $blue solid

.blue-box-pullquote
	background: $blue
	padding: 30px
	h1,h2,h3,h4,h5,h6,p,span
		line-height: 45px
		color: $white !important



// --- About page
#about
	#about-content
		.row:not(:first-child)
			margin-top: 1.85714286em


// --- Inspiration page
.inspiration-post
	width: 50%
	padding: 0 !important
	display: inline-block
#text-overlap, .text-overlap
	position: relative
	top -99px
	padding: 25px
	z-index: 3
	min-height: 100px
#inspiration-posts
	position: relative
	z-index: 3
	top -250px
	margin-bottom -200px
	.masonry-content
		padding: 30px
	.masonry__container
		.masonry__item
			transition: 0.2s
			margin: 0
			h1,h2,h3,h4,h5,h6
				font-weight: 700
			.btn
				height: 40px
				width: 40px
				.btn__text
					display: none
					font-size: 20px
			.btn-circle
				padding: 2px 20px !important
			&:first-child
				margin-top: 60px
			&:hover
				background: $blue
				transition: 0.2s
				box-shadow: $box-shadow
				//filter drop-shadow(0px 0px 10px rgba(0,0,0,0.25))
				.img-clip:after
					transition: 0.2s
					content ""
					width: 25%
					height: 25%
					background: transparent url("../img/corner-blue.png") no-repeat
					background-size: contain
					background-position: bottom right
					bottom: 0
					right: 0
					position: absolute
					display: block
					opacity: 1
				p, h1,h2,h3,h4,h5,h6
					color: $white !important
				.btn-circle
					height: 40px
					width: auto
					transition: padding 0s
					padding: 2px 20px !important
				.btn
					background: $white
					border-color: $white !important
					.btn__text
						padding-right: 20px
						color: $blue !important
						display: inline !important

.corner-point-green
	overflow: visible
	&:before
		content ""
		width: 25%
		height: 25%
		background: transparent url("../img/corner-green-2.png") no-repeat
		background-size: contain
		background-position: bottom left
		top -25%
		left: 0
		position: absolute
		display: block
		opacity: 1
.corner-point-blue
	overflow: visible
	&:before
		content ""
		width: 25%
		height: 25%
		background: transparent url("../img/corner-blue-2.png") no-repeat
		background-size: contain
		background-position: bottom left
		top -25%
		left: 0
		position: absolute
		display: block
		opacity: 1
.corner-point-white
	overflow: visible
	&:before
		content ""
		width: 25%
		height: 25%
		background: transparent url("../img/corner2.png") no-repeat
		background-size: contain
		background-position: bottom left
		top -25%
		left: 0
		position: absolute
		display: block
		opacity: 1





